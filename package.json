{
  "name": "daily-app",
  "version": "1.0.0",
  "description": "Daily Now gathers the latest dev & tech posts in your new tab on Chrome and Firefox. Every single day new and updated development technologies are being released across the web. For us as developers, staying updated is a huge hassle. Don’t bother looking anywhere else, simply install the extension, open a new tab and you’re all set!",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build && cp _redirects dist/",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "svg": "vsvg -s ./orig_icons -t ./icons"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^2.0.8",
    "intersection-observer": "^0.5.0",
    "localforage": "^1.7.2",
    "mixpanel-browser": "^2.22.4",
    "nuxt": "^1.4.2",
    "vue-svgicon": "^3.1.0",
    "vuex": "^3.0.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "cross-env": "^5.0.1",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "postcss-extend": "^1.0.5",
    "postcss-preset-env": "^5.3.0"
  }
}
